<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPaneMain" prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chinaunicom.elemeetingpc.controllers.LoginController">
   <center>
      <VBox id="mainBackground" alignment="CENTER" BorderPane.alignment="CENTER" fillWidth="false" prefHeight="700.0" prefWidth="1300.0">
         <children>
            <ImageView fitHeight="213.0" fitWidth="158.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="images/logo-2x(1).png" />
               </image>
            </ImageView>
            <HBox prefHeight="37.0" prefWidth="319.0">
               <children>
                  <ImageView fitHeight="36.0" fitWidth="46.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="images/loginName@2x.png" />
                     </image>
                  </ImageView>
                  <TextField fx:id="loginNameField" maxWidth="Infinity" prefHeight="36.0" prefWidth="280.0" promptText="%login.userNamePrompt" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="37.0" prefWidth="319.0">
               <children>
                  <ImageView fitHeight="36.0" fitWidth="46.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="images/loginPwd@2x.png" />
                     </image>
                  </ImageView>
                  <PasswordField fx:id="tpasswordfField" maxWidth="Infinity" prefHeight="36.0" prefWidth="280.0" promptText="%login.passwordPrompt" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </HBox>
            <Button alignment="CENTER" maxWidth="Infinity" mnemonicParsing="false" onAction="#showRegiCode" prefHeight="26.0" prefWidth="115.0" text="%login.verificationCode" textFill="#0d7ec9">
               <VBox.margin>
                  <Insets left="200.0" top="-10.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" maxWidth="Infinity" mnemonicParsing="false" onAction="#login" prefHeight="36.0" prefWidth="320.0" styleClass="login-btn" text="%login.loginButton" textFill="WHITE">
               <VBox.margin>
                  <Insets top="16.0" />
               </VBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
